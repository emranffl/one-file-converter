{
  "name": "one-file-converter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "NODE_OPTIONS='--max-old-space-size=6144 --trace-warnings --inspect --trace-uncaught' rm -rf .next",
    "dev": "next dev --turbopack",
    "prebuild": "rm -rf .next",
    "build": "NODE_ENV=production next build",
    "clean-install": "rm -rf node_modules pnpm-lock.yaml yarn.lock bun.lockb package-lock.json && pnpm install",
    "clean-build": "pnpm run clean-install && pnpm build",
    "start": "NODE_ENV=production next start",
    "pre-commit": "lint-staged --allow-empty",
    "format": "prettier --write .",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs,css,scss,json,html}": [
      "prettier --write"
    ]
  },
